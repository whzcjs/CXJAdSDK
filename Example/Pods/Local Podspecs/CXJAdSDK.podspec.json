  "source": {
    "git": "https://github.com/whzcjs/CXJAdSDK.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "AdSupport",
    "CoreTelephony",
    "SystemConfiguration"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "arm64 armv7 x86_64"
  },
  "default_subspecs": "CXJAdSDK",
  "subspecs": [
    {
      "name": "CXJAdSDK",
      "vendored_frameworks": [
        "CXJAdSDK/Frameworks/CXJAdSDK.xcframework",
        "CXJAdSDK/Frameworks/CXAdSDK.xcframework"
      ],
      "resources": "CXJAdSDK/Resources/CXAdsBundle.bundle",
      "public_header_files": "CXJAdSDK/Frameworks/CXJAdSDK.xcframework/**/Headers/*.h"
    },
    {
      "name": "CXBDAdapter",
      "vendored_frameworks": [
        "CXJAdSDK/Frameworks/CXBDAdapter.xcframework"
      ],
      "dependencies": {
        "BaiduMobAdSDK": []
      }
    },
    {
      "name": "CXFWAdapter",
      "vendored_frameworks": [
        "CXJAdSDK/Frameworks/CXFWAdapter.xcframework"
      ],
      "dependencies": {
        "JADYun": []
      }
    },
    {
      "name": "CXJADAdapter",
      "vendored_frameworks": [
        "CXJAdSDK/Frameworks/CXJADAdapter.xcframework"
      ],
      "dependencies": {
        "PTGAdFramework": []
      }
    },
    {
      "name": "CXQMAdapter",
      "vendored_frameworks": [
        "CXJAdSDK/Frameworks/CXQMAdapter.xcframework"
      ],
      "dependencies": {
        "QuMengAdSDK": []
      }
    }
  ]
}
